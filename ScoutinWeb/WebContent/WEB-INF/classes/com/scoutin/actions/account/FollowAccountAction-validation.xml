<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE validators PUBLIC "-//Apache Struts//XWork Validator 1.0.3//EN"
		"http://struts.apache.org/dtds/xwork-validator-1.0.3.dtd">
<validators>
	<field name="followedAccountId">
		<field-validator type="fieldexpression">
			<param name="expression">!(followedAccountId == null or followedAccountId == "")</param>
			<message key="error.account.followaccount.followedAccountId.required">Please input followedAccountId</message>
		</field-validator>
	</field>
</validators>